    "react-localization": "^1.0.13",
    "react-preloadr": "^1.2.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7"
  },
