{
  "name": "fake-news-client",
  "version": "0.0.0-semver",
  "description": "Fake news client layer",
  "author": "Hendrik Volschenk <righteous.trespasser@gmail.com>",
  "contributors": [
    {
      "email": "righteous.trespasser@gmail.com",
      "name": "Hendrik Volschenk"
    }
  ],
  "scripts": {
    "build": "npm run webpack:vendor:prod && npm run webpack:prod",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:scripts": "eslint --ext .js --ext .jsx .",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "postinstall": "npm run webpack:vendor",
    "start": "webpack-dev-server --hot --config=./.webpack/webpack.config.js --cert=./.ssl/localhost.crt --key=./.ssl/localhost.key --https",
    "test": "npm run lint && npm run test:vulnerabilities && npm run test:scripts",
    "test:scripts": "jest --coverage --config=./jest.config.json",
    "test:vulnerabilities": "npm audit || true",
    "webpack:prod": "NODE_ENV=production webpack --config=./.webpack/webpack.config.prod.js",
    "webpack:profile": "webpack --config=./.webpack/webpack.config.js --profile --json > .webpack/webpack.profile.json",
    "webpack:profile:vendor": "webpack --config=./.webpack/webpack.config.dll.js --profile --json > .webpack/webpack.profile.vendor.json",
    "webpack:vendor": "NODE_ENV=development webpack --config=./.webpack/webpack.config.dll.js",
    "webpack:vendor:prod": "NODE_ENV=production webpack --config=./.webpack/webpack.config.prod.dll.js"
  },
  "keywords": [],
  "private": true,
  "dependencies": {
